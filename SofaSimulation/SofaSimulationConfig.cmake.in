# CMake package configuration file for SofaSimulation.
# It defines the following variables:
# - SofaSimulation_INCLUDE_DIRS
# - SofaSimulation_LIBRARIES

@PACKAGE_INIT@

set(SOFASIMULATION_INCLUDE_DIR "@PACKAGE_SOFASIMULATION_INCLUDE_DIR@")
set(SofaSimulation_TARGETS SofaSimulationCommon SofaSimulationTree SofaSimulationGraph)

# Transitive dependencies
find_package(SofaFramework REQUIRED)

if(NOT TARGET SofaSimulationCommon)
	include("${CMAKE_CURRENT_LIST_DIR}/SofaSimulationTargets.cmake")
endif()

check_required_components(${SofaSimulation_TARGETS})

# For compatibility only
set(SofaSimulation_LIBRARIES SofaSimulationCommon SofaSimulationTree SofaSimulationGraph)
set(SofaSimulation_INCLUDE_DIRS ${SOFASIMULATION_INCLUDE_DIR} ${SofaFramework_INCLUDE_DIRS})
