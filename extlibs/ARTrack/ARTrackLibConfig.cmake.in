# - Config file for the ARTrackLib package
# It defines the following variables
#  ARTrackLib_INCLUDE_DIRS  - include directories for ARTrackLib
#  ARTrackLib_LIBRARIES     - libraries to link against

@PACKAGE_INIT@

set_and_check(ARTRACK_INCLUDE_DIR "@PACKAGE_ARTRACK_INCLUDE_DIR@")

if(NOT TARGET ARTrackLib)
	include("${CMAKE_CURRENT_LIST_DIR}/ARTrackLibTargets.cmake")
endif()

check_required_components(ARTrackLib)

set(ARTrackLib_LIBRARIES ARTrackLib)
set(ARTrackLib_INCLUDE_DIRS ${ARTRACK_INCLUDE_DIR})
