# CMake package configuration file for the SofaMiscFem module
@PACKAGE_INIT@

set(SOFAMISCFEM_HAVE_NEWMAT @Newmat_FOUND@)

find_package(SofaBase REQUIRED)

if(SOFAMISCFEM_HAVE_NEWMAT)
	find_package(Newmat REQUIRED)
endif()

### Is the target existing ?
if(NOT TARGET SofaMiscFem)
    ### If not we are building out of tree.
    include("${CMAKE_CURRENT_LIST_DIR}/SofaMiscFem.cmake")
endif()

check_required_components(SofaMiscFem)
