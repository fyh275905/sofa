# CMake package configuration file for the @PROJECT_NAME@ module

@PACKAGE_GUARD@
@PACKAGE_INIT@

set(SOFAGUIMAIN_HAVE_SOFAGUIQT @SOFAGUIMAIN_HAVE_SOFAGUIQT@)
set(SOFAGUIMAIN_HAVE_SOFAHEADLESSRECORDER @SOFAGUIMAIN_HAVE_SOFAHEADLESSRECORDER@)

find_package(SofaGuiCommon QUIET REQUIRED)

if(SOFAGUIMAIN_HAVE_SOFAGUIQT)
    find_package(SofaGuiQt QUIET REQUIRED)
endif()

if(SOFAGUIMAIN_HAVE_SOFAHEADLESSRECORDER)
    find_package(SofaHeadlessRecorder QUIET REQUIRED)
endif()

if(NOT TARGET @PROJECT_NAME@)
    include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
endif()

check_required_components(@PROJECT_NAME@)
