# CMake package configuration file for the @PROJECT_NAME@ module

@PACKAGE_GUARD@
@PACKAGE_INIT@

set(SOFAGENERALENGINE_HAVE_SOFA.GL @SOFAGENERALENGINE_HAVE_SOFA.GL@)

find_package(SofaMeshCollision QUIET REQUIRED)
