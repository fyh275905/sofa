# CMake package configuration file for the SofaMiscMapping module
@PACKAGE_INIT@

find_package(SofaCommon REQUIRED)
find_package(SofaGeneral REQUIRED)

### Is the target existing ?
if(NOT TARGET SofaMiscMapping)
    ### If not we are building out of tree.
    include("${CMAKE_CURRENT_LIST_DIR}/SofaMiscMapping.cmake")
endif()

check_required_components(SofaMiscMapping)
