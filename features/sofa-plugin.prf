!contains(TEMPLATE, lib): error(plugins should have the "lib" qmake template)

!contains(CONFIGSTATIC, static) {
	CONFIG -= staticlib
	CONFIG += dll
}

equals(USING_INSTALL, true) {
	INSTALL_INC_PATH = $$INSTALL_INC_PATH/applications/plugins/$$basename(ROOT_SRC_DIR)
	INSTALL_INC_DIR = $$INSTALL_DIR/$$INSTALL_INC_PATH
}
