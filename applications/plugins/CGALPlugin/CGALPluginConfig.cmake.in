# - Config file for the CGALPlugin package
# It defines the following variables
#  CGALPlugin_INCLUDE_DIRS - include directories for CGALPlugin
#  CGALPlugin_LIBRARIES    - libraries to link against

@PACKAGE_INIT@

set_and_check(CGALPLUGIN_INCLUDE_DIR  "@PACKAGE_CGALPLUGIN_INCLUDE_DIR@")

check_required_components(CGALPlugin)

find_package(SofaFramework REQUIRED)
find_package(CGAL REQUIRED)
find_package(MPFR REQUIRED)
find_package(GMP REQUIRED)
find_package(Boost REQUIRED COMPONENTS system thread)

if(NOT TARGET CGALPlugin)
	include("${CMAKE_CURRENT_LIST_DIR}/CGALPluginTargets.cmake")
endif() 

set(CGALPlugin_LIBRARIES CGALPlugin)
set(CGALPlugin_INCLUDE_DIRS ${CGALPLUGIN_INCLUDE_DIR})
