		<Node name="CM">
			<Object type="MechanicalObject" name="CollisionDOFs" template="Vec3" size="3"/>
			<Object type="LineSet" />
			<Object type="PointSet" />
			<Object type="IdentityMapping"/>
		</Node>
		
		<Node name="VM">
			<Object type="MechanicalObject" name="VisualDOFs"/>
