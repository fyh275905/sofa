			<Object type="Edge2QuadTopologicalMapping" nbPointsOnEachCircle="10" radius="0.5" object1="../../Container" object2="Container"/>
			<Object type="TubularMapping" nbPointsOnEachCircle="10" radius="0.5" object1="../../MechanicalDOFs" object2="../VisualDOFs" />
			
			<Node name="Visu">
				<Object type="OglModel" name="Visual" color="1.0 0.0 0.0 1.0" />
				<Object type="IdentityMapping" object1="../../VisualDOFs" object2="Visual" />
			</Node>			
		</Node>
				
	</Node>
	
