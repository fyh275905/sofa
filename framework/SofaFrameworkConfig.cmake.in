# - Config file for the SofaFramework package
# It defines the following variables
#  SofaFramework_INCLUDE_DIRS - include directories for SofaFramework
#  SofaFramework_LIBRARIES    - libraries to link against
 

@PACKAGE_INIT@

set_and_check(SOFAFRAMEWORK_INCLUDE_DIR          "@PACKAGE_SOFAFRAMEWORK_INCLUDE_DIR@")
set(SOFAFRAMEWORK_EXTERNAL_INCLUDE_DIR "@PACKAGE_SOFAFRAMEWORK_EXTERNAL_INCLUDE_DIR@")
set_and_check(SOFAFRAMEWORK_BIN_DIR              "@PACKAGE_SOFAFRAMEWORK_BIN_DIR@")

check_required_components(SofaFramework)

find_package(TinyXML REQUIRED)

include("${CMAKE_CURRENT_LIST_DIR}/SofaFrameworkTargets.cmake")

# These are IMPORTED targets created by SofaFrameworkTargets.cmake
set(SofaFramework_LIBRARIES SofaHelper SofaDefaultType SofaCore)
set(SofaFramework_INCLUDE_DIRS ${SOFAFRAMEWORK_INCLUDE_DIR} ${SOFAFRAMEWORK_EXTERNAL_INCLUDE_DIR} ${SOFAFRAMEWORK_BIN_DIR} )