# CMake package configuration file for SofaGeneral.
# It defines the following variables:
# - SofaGeneral_INCLUDE_DIRS
# - SofaGeneral_LIBRARIES

@PACKAGE_INIT@

set(SOFAGENERAL_INCLUDE_DIR "@PACKAGE_SOFAGENERAL_INCLUDE_DIR@")

# Transitive dependencies
find_package(SofaCommon REQUIRED)
find_package(Newmat)
find_package(CSparse)

set(SOFA_HAVE_NEWMAT @SOFA_HAVE_NEWMAT@)
set(SofaGeneral_TARGETS @SOFAGENERAL_TARGETS@)

if(SOFA_HAVE_NEWMAT)
    find_package(Newmat REQUIRED)
endif()


if(NOT TARGET SofaComponentGeneral)
	include("${CMAKE_CURRENT_LIST_DIR}/SofaGeneralTargets.cmake")
endif()

check_required_components(${SofaGeneral_TARGETS})

# For compatibility only
set(SofaGeneral_LIBRARIES SofaComponentGeneral)
set(SofaGeneral_INCLUDE_DIRS ${SOFAGENERAL_INCLUDE_DIR} ${SofaCommon_INCLUDE_DIRS})
