# - Config file for the SofaFramework package
# It defines the following variables
#  SofaSimulation_INCLUDE_DIRS - include directories for FooBar
#  SofaSimulation_LIBRARIES    - libraries to link against

@PACKAGE_INIT@

set(SOFAGENERAL_INCLUDE_DIR "@PACKAGE_SOFAGENERAL_INCLUDE_DIR@")
check_required_components(SofaGeneral)

# required for transitive dependencies
find_package(SofaCommon REQUIRED)

if( NOT TARGET SofaComponentGeneral )
	include("${CMAKE_CURRENT_LIST_DIR}/SofaGeneralTargets.cmake")
endif()
 
# For compatibility only
set(SofaGeneral_LIBRARIES @SOFAGENERAL_TARGETS@)
set(SofaGeneral_INCLUDE_DIRS ${SOFAGENERAL_INCLUDE_DIR} ${SofaCommon_INCLUDE_DIRS} )
