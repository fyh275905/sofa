<Node name="root" dt="0.05" showBehaviorModels="1" showCollisionModels="0" showMappings="0" showForceFields="0" showBoundingTree="0" gravity="0 0 0">
	<Object type="CollisionPipeline" verbose="0" />
	<Object type="BruteForceDetection" name="N2" />
	<Object type="CollisionResponse" response="default" />
	<Object type="MinProximityIntersection" name="Proximity" alarmDistance="0.8" contactDistance="0.5" />
	<Object type="CollisionGroup" />

	<Node name="TT">
		<EulerImplicit name="cg_odesolver" printLog="false"/>
		<CGLinearSolver iterations="25" name="linear solver" tolerance="1.0e-9" threshold="1.0e-9" />
		<MeshLoader name="meshLoader" filename="mesh/cubeDof.msh" />
		<MechanicalObject name="Volume" />
		<TetrahedronSetTopologyContainer name="Container" listening="1"/>
		<TetrahedronSetTopologyModifier name="Modifier" />
		<TetrahedronSetTopologyAlgorithms template="Vec3d" name="TopoAlgo" />
		<TetrahedronSetGeometryAlgorithms template="Vec3d" name="GeomAlgo" drawTetrahedra="1"/>
		<TopologicalChangeProcessor listening="1" filename="AddingTetrahedraProcess.txt"/>

		<UniformMass totalmass="1" />
		<FixedPlaneConstraint direction="0 0 1" dmin="-0.1" dmax="0.1"/>
		<FixedConstraint indices="0" />		
<!--        	<Object type="TetrahedralCorotationalFEMForceField" name="FEM" youngModulus="60" poissonRatio="0.3" method="large" />-->

		<Node name="T">
		  <TriangleSetTopologyContainer name="Container" listening="1"/>
		  <TriangleSetTopologyModifier name="Modifier" />
		  <TriangleSetTopologyAlgorithms template="Vec3d" name="TopoAlgo" />
		  <TriangleSetGeometryAlgorithms template="Vec3d" name="GeomAlgo" drawTriangles="1"/>
		  <Tetra2TriangleTopologicalMapping object1="../../Container" object2="Container" listening="1"/>
		  
            	  <!--<Object type="TriangularFEMForceField" name="FEM" youngModulus="10" poissonRatio="0.3" method="large" /> 
		  			<Object type="TriangularBendingSprings" name="FEM-Bend" stiffness="300" damping="1.0"/>		    
					<Object type="TrianglePressureForceField" normal="0 0 1" dmin="0.9" dmax="1.1" pressure="0.4 0 0"/> -->
		  <Object type="TriangleSet"/>
		  
<!--		  <Node name="Visu">
		    <Object type="OglModel" name="Visual" color="blue" />
		    <Object type="IdentityMapping" object1="../../../Volume" object2="Visual" />
		  </Node>						-->
		</Node>
	</Node>
</Node>


