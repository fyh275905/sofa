<Node name="MeshSpring">
	<ParallelCGImplicitSolver rayleighStiffness="0.01"  iterations="25" threshold="0.00000001" />	 
	<MeshLoader name="mechaLoader" filename="file"/>
	<Mesh/> 
	<MechanicalObject translation="0 0 0" rotation="0 0 0" scale3d="1.0 1.0 1.0"/>
	<UniformMass totalmass="1" />
	<MeshSpringForceField name="Springs" stiffness="400" damping="4"/>
	<Node name="VisualNode">
		<OglModel name="Visual" filename="file"  color="green"   translation="0 0 0" rotation="0 0 0" scale3d="1.0 1.0 1.0"/>
		<BarycentricMapping object1="../.." object2="Visual" />
	</Node>
	<Node name="CollisionNode">
    <MeshLoader name="collisionLoader"  filename="file"/>
		<Mesh />
		<MechanicalObject useMask="true"  translation="0 0 0" rotation="0 0 0" scale3d="1.0 1.0 1.0"/>
		<Triangle />
		<Line />
		<Point />
		<BarycentricMapping />
	</Node>
</Node>
