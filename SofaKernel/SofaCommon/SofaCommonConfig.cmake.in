# CMake package configuration file for SofaCommon

@PACKAGE_GUARD@
@PACKAGE_INIT@

set(SOFACOMMON_TARGETS @SOFACOMMON_TARGETS@ @SOFACOMMON_MODULES@)
set(SOFACOMMON_MODULES @SOFACOMMON_MODULES@)

find_package(SofaBase QUIET REQUIRED)

foreach(module ${SOFACOMMON_MODULES})
    find_package(${module} QUIET REQUIRED)
endforeach()

foreach(target ${SOFACOMMON_TARGETS})
    if(NOT TARGET ${target})
        include("${CMAKE_CURRENT_LIST_DIR}/SofaCommonTargets.cmake")
        break()
    endif()
endforeach()
if(NOT TARGET @PROJECT_NAME@)
    include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
endif()
check_required_components(@PROJECT_NAME@)
